1.
#pragma warning (disable: 4996)
#include<stdio.h>

int main() {
	int x[10];
	int i;
	for (i = 0; i < 10; i++) {
		scanf("%d", &x[i]);
	}
	for (i = 0; i < 10; i++) {
		if (x[i] % 2 == 0) {
			printf(" %d", x[i]);
		}
	}
	return 0;
}

2.
#pragma warning (disable: 4996)
#include<stdio.h>

int main() {
	int x[10];
	int i, sum = 0;
	for (i = 0; i < 10; i++) {
		scanf("%d", &x[i]);
	}
	for (i = 0; i < 10; i++) {
		sum += x[i];
	}
	printf("%d", sum);
	return 0;
}

